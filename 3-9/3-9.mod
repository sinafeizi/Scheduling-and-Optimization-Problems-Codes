int n=4;
range tedad= 1..n;
dvar float+ x[tedad];

maximize((4*x[1])+(3*x[2])+(-5*x[3])+(2*x[4]));

subject to
{
((7*x[1])+(-2*x[2])+(5*x[4]))<=11;
((-1*x[3])+(-1*x[4]))<=-3;
((x[1])+(2*x[2])+(5*x[3]))<=5;
x[2]<=2;
}
