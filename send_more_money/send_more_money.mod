using CP;
int n=8;                                                 //x[1]==S
range adad=1..n;                                         //x[2]==E
                                                         //x[3]==N
dvar int x[adad] in 0..9;                              //x[4]==D
                                                         //x[5]==M
                                                         //x[6]==O
                                                         //x[7]==R
subject to                                               //x[8]==Y

{
 allDifferent (all(i in adad) x[i]);
x[1]!=0;  
x[5]!=0;    
(1000*x[1])+(100*x[2])+(10*x[3])+(1*x[4])+(1000*x[5])+(100*x[6])+(10*x[7])+(1*x[2])==(10000*x[5])+(1000*x[6])+(100*x[3])+(10*x[2])+(1*x[8]);
}





